	<TerrainDef ParentName="FloorBase">
		<defName>VGE_GravshipSubscaffold</defName>
		<label>gravship subscaffold</label>
		<description>A scaffold layer built to support the framework of gravships. While part of the gravship’s structure, it cannot support buildings and slows movement significantly. It does not count toward the gravship’s substructure limit, making it ideal for walkways, access paths, or expansion staging areas.</description>
		<texturePath>Things/Terrain/Substructure/Subscaffolding</texturePath>
		<uiIconPath>UI/MenuIcons/Subscaffold_MenuIcon</uiIconPath>
		<fertility>0</fertility>
		<edgeType>Fade</edgeType>
		<avoidWander>true</avoidWander>
		<pathCost>9</pathCost>
		<renderPrecedence>400</renderPrecedence>
		<constructEffect>ConstructMetal</constructEffect>
		<isPaintable>true</isPaintable>
		<designationCategory>VGE_Platform</designationCategory>
		<isFoundation>true</isFoundation>
		<holdSnowOrSand>false</holdSnowOrSand>
		<preventCraters>true</preventCraters>
		<affordances Inherit="false">
			<li>Walkable</li>
		</affordances>
		<statBases>
			<WorkToBuild>300</WorkToBuild>
			<Flammability>0</Flammability>
			<CleaningTimeFactor>1.3</CleaningTimeFactor>
			<Beauty>0</Beauty>
			<MarketValue>6</MarketValue>
			<FilthMultiplier>0</FilthMultiplier>
		</statBases>
		<costList>
			<GravlitePanel>1</GravlitePanel>
		</costList>
		<uiOrder>1001</uiOrder>
		<terrainAffordanceNeeded>Walkable</terrainAffordanceNeeded>
		<resourcesFractionWhenDeconstructed>1</resourcesFractionWhenDeconstructed>
		<researchPrerequisites>
			<li>StandardGravtech</li>
		</researchPrerequisites>
		<placeWorkers>
			<li>PlaceWorker_InSubstructureFootprint</li>
		</placeWorkers>
		<tags>
			<li>Substructure</li>
		</tags>
	</TerrainDef>
