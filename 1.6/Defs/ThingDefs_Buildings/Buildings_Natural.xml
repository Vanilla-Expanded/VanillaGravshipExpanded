<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="UglyRockBase">
		<defName>VGE_Compressed_Vacstone</defName>
		<label>compressed vacstone</label>
		<description>An ultra-dense mass of asteroid rock and dust, compacted over millennia into a near-indestructible block. Found only in deep space.</description>
		<graphicData>
			<texPath>Things/Natural/DeepRockWallFlecked_Atlas</texPath>
			<color>(115,106,98)</color>
		</graphicData>
		<statBases>
			<MaxHitPoints>3000</MaxHitPoints>
		</statBases>
		<building>
			<mineableThing>ChunkVacstone</mineableThing>
			<mineableDropChance>1</mineableDropChance>
			<mineablePreventMeteorite>true</mineablePreventMeteorite>
			<mineablePreventNaturalRockOnSurface>true</mineablePreventNaturalRockOnSurface>
			<naturalTerrain>VGE_Compressed_Vacstone_Floor</naturalTerrain>
			<leaveTerrain>VGE_Compressed_Vacstone_Floor</leaveTerrain>
		</building>
	</ThingDef>
	<ThingDef ParentName="RockBase">
		<defName>VGE_Compressed_Steel</defName>
		<label>compressed steel</label>
		<graphicData>
			<texPath>Things/Natural/DeepRockWallFlecked_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<color>(115,106,98)</color>
			<colorTwo>(155,133,107)</colorTwo>
		</graphicData>
		<statBases>
			<MaxHitPoints>7500</MaxHitPoints>
		</statBases>
		<description>Ancient structural mass fused into a single, ultra-dense vacstone-steel deposit. Found only in deep space.</description>
		<building>
			<isResourceRock>true</isResourceRock>
			<mineableThing>Steel</mineableThing>
			<mineableYield>80</mineableYield>
			<mineableScatterCommonality>0.02</mineableScatterCommonality>
			<mineableScatterLumpSizeRange>
				<min>75</min>
				<max>120</max>
			</mineableScatterLumpSizeRange>
			<veinMineable>true</veinMineable>
			<mineablePreventNaturalRockOnSurface>true</mineablePreventNaturalRockOnSurface>
		</building>
	</ThingDef>
