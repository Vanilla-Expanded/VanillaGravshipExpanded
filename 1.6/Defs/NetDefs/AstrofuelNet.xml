          <texPath>UI/Overlays/Overlay_Oxygen</texPath>
          <labelKey>VGE_ExtractAstrofuel</labelKey>
          <descKey>VGE_ExtractAstrofuelDesc</descKey>
          <extractAmount>150</extractAmount>
          <thing>VGE_Astrofuel</thing>
          <ratio>1</ratio>
        </extractOptions>
        <refillOptions>
          <thing>VGE_Astrofuel</thing>
        </refillOptions>
