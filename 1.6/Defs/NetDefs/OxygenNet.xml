  <ThingDef ParentName="BuildingBase">
    <defName>VGSE_SmallOxygenTank</defName>
    <label>small oxygen tank</label>
    <description>An oxygen tank that supplies oxygen to pumps throughout the gravship.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/PipeNetworks/Oxygen/OxygenTank</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.40</fillPercent>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToBuild>2250</WorkToBuild>
      <Mass>25</Mass>
      <Flammability>0.6</Flammability>
    </statBases>
    <size>(2,2)</size>
    <costList>
      <Steel>100</Steel>
    </costList>
    <comps>
      <li Class="PipeSystem.CompProperties_ResourceStorage">
        <pipeNet>VGSE_OxygenNet</pipeNet>
        <storageCapacity>200</storageCapacity>
        <barSize>(0.8, 0.15)</barSize>
        <margin>0.1</margin>
        <centerOffset>(0, 0, -0.5)</centerOffset>
        <barHorizontal>true</barHorizontal>
        <rotateBarWithBuilding>false</rotateBarWithBuilding>
        <extractOptions>
          <!-- TODO: Add actual icon -->
          <texPath>UI/Overlays/Overlay_Oxygen</texPath>
          <labelKey>VGSE_ExtractOxygen</labelKey>
          <descKey>VGSE_ExtractOxygenDesc</descKey>
          <extractAmount>10</extractAmount>
          <thing>VGSE_OxygenCanister</thing>
          <ratio>1</ratio>
        </extractOptions>
      </li>
      <li Class="PipeSystem.CompProperties_ExplosiveContent">
        <explosiveMaxRadius>7</explosiveMaxRadius>
        <radiusRequiredForExplosion>2</radiusRequiredForExplosion>
        <!-- Vanilla options below -->
        <explosiveDamageType>Thump</explosiveDamageType>
        <damageAmountBase>9</damageAmountBase>
        <destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
        <startWickHitPointsPercent>0.2</startWickHitPointsPercent>
        <wickTicks>70~150</wickTicks>
      </li>
    </comps>
    <terrainAffordanceNeeded>Substructure</terrainAffordanceNeeded>
    <designationCategory>Odyssey</designationCategory>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>BiofuelRefining</li>
    </researchPrerequisites>
    <disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
  </ThingDef>

