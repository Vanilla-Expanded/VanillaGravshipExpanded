  <ThingDef ParentName="FurnitureBase">
    <defName>VGSE_OxygenCanisterFiller</defName>
    <label>oxygen canister filler</label>
    <description>A hopper-like storage device meant to hold dispensed oxygen in canisters for use later.</description>
    <thingClass>Building_Storage</thingClass>
    <minifiedDef IsNull="True" />
    <thingCategories Inherit="False" />
    <graphicData>
      <texPath>Things/PipeNetworks/Oxygen/OxygenCanisterFiller</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
      <drawRotated>false</drawRotated>
      <allowFlip>false</allowFlip>
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.5</fillPercent>
    <castEdgeShadows>true</castEdgeShadows>
    <pathCost>50</pathCost>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>600</WorkToBuild>
      <Flammability>0.7</Flammability>
      <Beauty>-2</Beauty>
    </statBases>
    <size>(1,1)</size>
    <costList>
      <Steel>50</Steel>
    </costList>
    <building>
      <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <fixedStorageSettings>
        <filter>
          <disallowNotEverStorable>true</disallowNotEverStorable>
          <thingDefs>
            <li>VGSE_OxygenCanister</li>
          </thingDefs>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Important</priority>
        <filter>
          <disallowNotEverStorable>true</disallowNotEverStorable>
          <thingDefs>
            <li>VGSE_OxygenCanister</li>
          </thingDefs>
        </filter>
      </defaultStorageSettings>
    </building>
    <inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <designationCategory>Odyssey</designationCategory>
    <staticSunShadowHeight>0.5</staticSunShadowHeight>
    <surfaceType>Item</surfaceType>
    <canOverlapZones>false</canOverlapZones>
    <comps>
      <li Class="PipeSystem.CompProperties_ConvertResourceToThing">
        <pipeNet>VGSE_OxygenNet</pipeNet>
        <thing>VGSE_OxygenCanister</thing>
      </li>
    </comps>
    <researchPrerequisites>
      <li>BiofuelRefining</li>
    </researchPrerequisites>
  </ThingDef>

