  <ThingDef ParentName="BuildingBase">
    <defName>VGSE_OxygenHarvester</defName>
    <label>oxygen harvester</label>
    <description>A device that draws in breathable oxygen from the surrounding atmosphere and stores it in the oxygen network. It functions only on planetary surfaces with an atmosphere and will not operate in vacuum or low-oxygen environments.</description>
    <graphicData>
      <texPath>Things/Structures/OxygenScrubber/OxygenScrubber</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3,3)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <blockWind>false</blockWind>
    <fillPercent>0.4</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.4</staticSunShadowHeight>
    <statBases>
      <WorkToBuild>10000</WorkToBuild>
      <MaxHitPoints>250</MaxHitPoints>
      <Flammability>0.5</Flammability>
      <Beauty>-8</Beauty>
    </statBases>
    <size>(2,2)</size>
    <tickerType>Rare</tickerType>
    <costList>
      <Steel>200</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>700</basePowerConsumption>
        <idlePowerDraw>20</idlePowerDraw>
      </li>
      <li Class="VanillaGravshipExpanded.CompProperties_ResourceTrader_NotInVacuumBiome">
        <compClass>VanillaGravshipExpanded.CompResourceTrader_NotInVacuumBiome</compClass>
        <lowOxygenEnvironmentOverlayPath>UI/Overlays/Overlay_LackingAtmosphere</lowOxygenEnvironmentOverlayPath>
        <resourceOffOverlay>false</resourceOffOverlay>
        <producerLowPowerWhenStorageFull>true</producerLowPowerWhenStorageFull>
        <soundAmbientReceivingResource>GeothermalPlant_Ambience</soundAmbientReceivingResource>
        <pipeNet>VGSE_OxygenNet</pipeNet>
        <consumptionPerTick>-1</consumptionPerTick>
        <idleConsumptionPerTick>-1</idleConsumptionPerTick>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <designationCategory>Odyssey</designationCategory>
    <researchPrerequisites>
      <li>BiofuelRefining</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
    <rotatable>false</rotatable>
  </ThingDef>

