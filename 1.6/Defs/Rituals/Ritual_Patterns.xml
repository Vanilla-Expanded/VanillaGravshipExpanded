<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RitualPatternDef Abstract="True" Name="VGE_GravshipLaunchPattern">
    <beginRitualOverride>launch...</beginRitualOverride>
    <ritualOnlyForIdeoMembers>false</ritualOnlyForIdeoMembers>
    <playsIdeoMusic>false</playsIdeoMusic>
    <alwaysStartAnytime>true</alwaysStartAnytime>
    <ignoreExtremeTemperatures>true</ignoreExtremeTemperatures>
    <showIdeoIconsInDialog>false</showIdeoIconsInDialog>
  </RitualPatternDef>

  <RitualPatternDef ParentName="VGE_GravshipLaunchPattern">
    <defName>VGE_GravjumperLaunch</defName>
    <ritualBehavior>VGE_GravjumperLaunch</ritualBehavior>
    <ritualOutcomeEffect>VGE_GravjumperLaunch</ritualOutcomeEffect>
    <ritualObligationTargetFilter>VGE_GravjumperLaunch</ritualObligationTargetFilter>
    <iconPathOverride>UI/RitualIcons/LaunchGravjumper_Gizmo</iconPathOverride>
    <cancelIconPathOverride>UI/RitualIcons/CancelLaunchGravjumper_Gizmo</cancelIconPathOverride>
  </RitualPatternDef>

  <RitualPatternDef ParentName="VGE_GravshipLaunchPattern">
    <defName>VGE_GravhulkLaunch</defName>
    <ritualBehavior>VGE_GravhulkLaunch</ritualBehavior>
    <ritualOutcomeEffect>VGE_GravhulkLaunch</ritualOutcomeEffect>
    <ritualObligationTargetFilter>VGE_GravhulkLaunch</ritualObligationTargetFilter>
    <iconPathOverride>UI/RitualIcons/LaunchGravhulk_Gizmo</iconPathOverride>
    <cancelIconPathOverride>UI/RitualIcons/CancelLaunchGravhulk_Gizmo</cancelIconPathOverride>
  </RitualPatternDef>

</Defs>